    <!-- Landing Page start -->
    <section class="home" id="home">
      <div class="content">
        <h2 data-aos="fade-up">Flamstead Dry Cleaning</h2>
        <p data-aos="fade-up" data-aos-delay="100">
          Expert Care for Your Precious Garments<br>
          Refreshing Quality, Every Time
        </p>
        <a href="#service" class="btn-1-rails" data-aos="fade-up" data-aos-delay="200">Book Now</a>
      </div>

    </section>
